{
  "name": "@ironkinoko/linovelib-scan",
  "version": "1.0.8",
  "description": "download linovelib as epub",
  "scripts": {
    "dev": "nodemon -w src -e \"*\" -x ts-node-esm src/cli.ts 2923 第三卷",
    "build": "rm -rf dist && tsc && cp -r src/epub/templates dist/epub/templates",
    "epub":"NODE_ENV=development ts-node-esm src/cli.ts",
    "release": "pnpm build && npm publish",
    "test": "node --experimental-vm-modules --no-warnings node_modules/jest/bin/jest.js",
    "test:coverage": "pnpm test -- --coverage"
  },
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "bin": {
    "linovelib-scan": "./src/cli.ts"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IronKinoko/linovelib-scan.git"
  },
  "bugs": {
    "url": "https://github.com/IronKinoko/linovelib-scan/issues"
  },
  "homepage": "https://github.com/IronKinoko/linovelib-scan#readme",
  "keywords": [],
  "author": "IronKinoko <kinoko_main@outlook.com>",
  "license": "MIT",
  "dependencies": {
    "archiver": "^5.3.1",
    "axios": "^1.1.2",
    "cheerio": "1.0.0-rc.12",
    "fs-extra": "^10.1.0",
    "mime-types": "^2.1.35",
    "mustache": "^4.2.0"
  },
  "devDependencies": {
    "@types/archiver": "^5.3.1",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^29.1.2",
    "@types/mime-types": "^2.1.1",
    "@types/mustache": "^4.2.1",
    "@types/node": "^18.8.3",
    "jest": "^29.1.2",
    "nodemon": "^2.0.20",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
